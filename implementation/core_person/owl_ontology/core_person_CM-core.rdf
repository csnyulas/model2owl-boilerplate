   <owl:ObjectProperty rdf:about="http://data.europa.eu/a4g/ontology#change"/>
   <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#change">
      <skos:prefLabel xml:lang="en">Change</skos:prefLabel>
   </rdf:Description>
   <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#change">
      <label-en xmlns="" xml:lang="en">change</label-en>
   </rdf:Description>
   <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#change">
      <usageNote-en xmlns="" xml:lang="en">For example, a change of name or a person's their death. It is assumed that an inverse relationship will also be defined.</usageNote-en>
   </rdf:Description>
   <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#change">
